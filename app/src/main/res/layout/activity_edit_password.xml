<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editPasswordVM" type="com.examonline.app.modules.editpassword.data.viewmodel.EditPasswordVM"/>
    </data>
    <FrameLayout android:id="@+id/frame"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStyle">
        <LinearLayout android:id="@+id/linear"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:id="@+id/toolbar"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linear1"
                    android:layout_gravity="center"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/image"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_43"/>

                    <TextView
                        android:id="@+id/txtResetPassword"
                        style="@style/txtUbuntumedium18"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_164pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_weight="0.8"
                        android:gravity="top|start"
                        android:text="@string/lbl_reset_password" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/image4"
                    imageRes="@{@drawable/img_group_1795}"
                    android:layout_width="@dimen/_600pxh"
                    android:layout_height="@dimen/_800pxh"
                    android:layout_gravity="center_horizontal"
                    tools:src="@drawable/img_group_1795" />
                <LinearLayout android:id="@+id/linear2"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical">
                    <LinearLayout android:id="@+id/linear3"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_marginEnd="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical">
                        <LinearLayout android:id="@+id/frame1"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:orientation="vertical">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtCurrentPasswor"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_marginStart="@dimen/_8pxh"
                                style="@style/txtUbuntumedium16"
                                android:text="@string/msg_current_passwor"/>
                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/passwordLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_8pxh"
                                app:boxStrokeWidth="0dp"
                                app:boxStrokeWidthFocused="0dp"
                                app:endIconTint="@color/gray_500"
                                app:endIconMode="password_toggle"
                                app:startIconDrawable="@drawable/img_lock"
                                app:startIconTint="@color/gray_500"
                                app:endIconDrawable="@drawable/show_password_selector"
                                >
                                <EditText
                                    android:id="@+id/editCurrentPass"
                                    android:layout_gravity="center"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30pxv"
                                    android:paddingStart="@dimen/_10pxh"
                                    android:paddingEnd="@dimen/_50pxh"
                                    android:paddingTop="@dimen/_10pxh"
                                    android:textColor="@color/blue_600"
                                    android:singleLine="true"
                                    android:maxLength="20"
                                    android:inputType="textPassword"
                                    android:background="@drawable/rectangle_bg_blue_600_00_border_blue_600_00_radius_10"
                                />
                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linear4"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <LinearLayout android:id="@+id/frame2"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:orientation="vertical">
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtNewPassword"
                                    android:layout_gravity="top|start"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_8pxh"
                                    android:layout_marginEnd="@dimen/_203pxh"
                                    android:layout_width="wrap_content"
                                    style="@style/txtUbuntumedium16" android:text="@string/lbl_new_password"/>
                                <com.google.android.material.textfield.TextInputLayout
                                    android:id="@+id/passwordLayout2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginStart="@dimen/_8pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_marginEnd="@dimen/_8pxh"
                                    app:boxStrokeWidth="0dp"
                                    app:boxStrokeWidthFocused="0dp"
                                    app:endIconTint="@color/gray_500"
                                    app:endIconMode="password_toggle"
                                    app:startIconDrawable="@drawable/img_lock"
                                    app:startIconTint="@color/gray_500"
                                    app:endIconDrawable="@drawable/show_password_selector"
                                    >
                                    <EditText
                                        android:id="@+id/editNewPass"
                                        android:layout_height="@dimen/_30pxv"
                                        android:layout_width="match_parent"
                                        android:paddingStart="@dimen/_10pxh"
                                        android:paddingTop="@dimen/_10pxh"
                                        android:paddingEnd="@dimen/_50pxh"
                                        android:maxLength="20"
                                        android:textColor="@color/blue_600"
                                        android:layout_gravity="center"
                                        android:singleLine="true"
                                        android:inputType="textPassword"
                                        android:background="@drawable/rectangle_bg_blue_600_00_border_blue_600_00_radius_10"
                                    />
                                </com.google.android.material.textfield.TextInputLayout>
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtNewPasswordMu"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_6pxv"
                                    android:layout_marginStart="@dimen/_8pxh"
                                    android:layout_width="match_parent"
                                    android:text="@{editPasswordVM.editPasswordModel.txtNewPasswordMu}"
                                    style="@style/txt_8" tools:text="@string/msg_new_password_mu"/>
                            </LinearLayout>

                        </LinearLayout>
                        <LinearLayout android:id="@+id/frame3"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical">
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtConfirmPasswor"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_174pxh"
                                android:layout_width="wrap_content"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:text="@{editPasswordVM.editPasswordModel.txtConfirmPasswor}"
                                style="@style/txtUbuntumedium16" tools:text="@string/msg_confirm_passwor"/>
                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/passwordLayout3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_8pxh"
                                app:boxStrokeWidth="0dp"
                                app:boxStrokeWidthFocused="0dp"
                                app:endIconTint="@color/gray_500"
                                app:endIconMode="password_toggle"
                                app:startIconDrawable="@drawable/img_lock"
                                app:startIconTint="@color/gray_500"
                                app:endIconDrawable="@drawable/show_password_selector"
                                >
                                <EditText
                                    android:id="@+id/editConfirmPass"
                                    android:layout_height="@dimen/_30pxv"
                                    android:layout_width="match_parent"
                                    android:paddingStart="@dimen/_10pxh"
                                    android:paddingTop="@dimen/_10pxh"
                                    android:maxLength="20"
                                    android:paddingEnd="@dimen/_50pxh"
                                    android:layout_gravity="center"
                                    android:background="@drawable/rectangle_bg_blue_600_00_border_blue_600_00_radius_10"
                                    android:textColor="@color/blue_600"
                                    android:singleLine="true"
                                    android:inputType="textPassword"
                                    />
                            </com.google.android.material.textfield.TextInputLayout>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:gravity="center"
                            android:id="@+id/btnSave"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_89pxh"
                            android:layout_marginStart="@dimen/_90pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="match_parent"
                            android:text="@string/lbl_save"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_save"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
